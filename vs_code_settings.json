    "cSpell.enableFiletypes": [
        "pgsql"
    ],
    "C_Cpp.updateChannel": "Insiders",
    "editor.minimap.enabled": false,
    "editor.matchBrackets": false,
    "go.toolsManagement.autoUpdate": true,
    // "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2 }",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "files.autoSave": "onFocusChange",
    "cmake.configureOnOpen": false,
    "yaml.schemas": {
        "file:///Users/tom/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.bitbucket.enabled": true,
    "atlascode.jira.enabled": false,
    "redhat.telemetry.enabled": true,
    "atlascode.bitbucket.explorer.refreshInterval": 5,
    "workbench.activityBar.visible": false,
    "editor.inlineSuggest.enabled": true,
    "workbench.colorTheme": "GitHub Light",
    "workbench.startupEditor": "none",
    "editor.bracketPairColorization.enabled": false,
    "window.zoomLevel": -1
}
